{
  "name": "@cto.af/editorconfig",
  "version": "0.15.3",
  "description": "EditorConfig File Locator and Interpreter for Node.js",
  "keywords": [
    "editorconfig",
    "core"
  ],
  "main": "lib/index.js",
  "contributors": [
    "Hong Xu (topbug.net)",
    "Jed Mao (https://github.com/jedmao/)",
    "Trey Hunner (http://treyhunner.com)",
    "Joe Hildebrand (https://github.com/hildjj/)"
  ],
  "directories": {
    "bin": "./bin",
    "lib": "./lib"
  },
  "scripts": {
    "clean": "rimraf lib",
    "parser": "peggy src/iniFile.peggy -o lib/iniFile.js",
    "prebuild": "npm run clean",
    "build": "tsc && npm run parser",
    "prebuild:map": "npm run clean",
    "build:map": "tsc --inlineSourceMap && npm run parser -- -m inline",
    "pretest": "npm run lint && npm run build && cmake .",
    "test": "npm run test:all",
    "test:all": "npm run test:unit && ctest --preset Test .",
    "test:unit": "mocha lib/*.test.js",
    "precoverage": "npm run build:map",
    "coverage": "c8 npm run test:all",
    "pretest:ci": "npm run pretest",
    "test:ci": "npm run test:all -- -VV",
    "lint": "npx eslint . --ext ts",
    "prepub": "npm run lint && npm run build",
    "pub": "npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/hildjj/ctoaf-editorconfig.git"
  },
  "bugs": "https://github.com/hildjj/ctoaf-editorconfig/issues",
  "author": "EditorConfig Team",
  "license": "MIT",
  "dependencies": {
    "commander": "^9.4.0",
    "minimatch": "^5.1.0",
    "semver": "^7.3.7"
  },
  "devDependencies": {
    "@types/minimatch": "^5.1.2",
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.7.13",
    "@types/semver": "^7.3.12",
    "@typescript-eslint/eslint-plugin": "5.36.1",
    "@typescript-eslint/parser": "5.36.1",
    "c8": "7.12.0",
    "eslint": "8.23.0",
    "eslint-plugin-jsdoc": "39.3.6",
    "mocha": "^10.0.0",
    "peggy": "2.0.1",
    "rimraf": "^3.0.2",
    "should": "^13.2.3",
    "source-map-support": "0.5.21",
    "typescript": "^4.7.4"
  },
  "engines": {
    "node": ">=12.22"
  }
}
